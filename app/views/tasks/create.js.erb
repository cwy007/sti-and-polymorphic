$('#no-tasks').addClass('d-none');

<% if @task.complete? %>
  $('#js-complete-tasks').append('<%= j render @task %>');
<% else %>
  $('#js-incomplete-tasks').append('<%= j render @task %>');
<% end %>

$('#edit_task_<%= @task.id %>').find('input[type=checkbox]').click(function() {
  $(this).parent('label').parent('form').submit();
  console.log(this);
});

$('#new_task #task_name').val('');
